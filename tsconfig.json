{
  "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "ESNext",
      "moduleResolution": "node",
      "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}